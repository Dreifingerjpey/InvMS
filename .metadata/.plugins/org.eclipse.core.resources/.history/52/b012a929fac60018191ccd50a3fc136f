package Main;
import Funktionen.Funktion;
import Suchfunktionen.Schwarmverfahren;
import Suchfunktionen.SuchAlgorithmus;


public class InversionMain {

	public static void main (String[] args) {

		
		 String funktion = "Poisson";
		 
		 Funktion f 		 =	new Funktionen.Poisson(funktion,1.2F); // lambda = 1,2
		 SuchAlgorithmus s	 =	new Schwarmverfahren();
		 Ergebnis ergebnis  =	s.execute();
		 
		 System.out.println("Für die Funktion: "+f.toString()+" wurde Berechnet:\n"+ergebnis.toString());
		
	}
	
}
